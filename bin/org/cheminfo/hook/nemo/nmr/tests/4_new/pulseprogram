# 12 "C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30" 2


# 15 "C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30"
"acqt0=-p1*2/3.1416"


# 1 "mc_line 18 file C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30 exp. def. part of mc cmd. before ze"
define delay MCWRK
define delay MCREST
"MCREST = 30m - 30m"
"MCWRK = 0.333333*30m"

    dccorr
# 18 "C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30"
1 ze
# 1 "mc_line 18 file C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30 exp. def. of mc cmd. after ze"
# 19 "C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30"
# 1 "mc_line 19 file C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30 exp. start label for mc cmd."
2 MCWRK  * 2
LBLF0, MCWRK
  MCREST
# 20 "C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30"
  d1
  p1*0.33 ph1
  go=2 ph31
# 1 "mc_line 23 file C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30 exp. mc cmd. in line"
  MCWRK wr #0
  MCWRK zd
  lo to LBLF0 times td0

  MCWRK
# 24 "C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30"
exit
# 27 "C:/Bruker/TopSpin3.2.pl6_RxM/exp/stan/nmr/lists/pp/zg30"
ph1=0 2 2 0 1 3 3 1
ph31=0 2 2 0 1 3 3 1


;pl1 : f1 channel - power level for pulse (default)
;p1 : f1 channel -  high power pulse
;d1 : relaxation delay; 1-5 * T1
;NS: 1 * n, total number of scans: NS * TD0
